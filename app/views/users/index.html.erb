<h1>Users Display</h1>
<table>
  <thead>
    <th>Name</th>
    <th>Email</th>
    <th>Action</th>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td><%= user.email %></td>
        <td><%= link_to "Show", user_path(user) %></td>
        <td><%= link_to  "Edit", edit_user_path(user) %></td>
        <td><%= link_to  "Delete", user_path(user), method: :delete %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= link_to  "New", new_user_path %>